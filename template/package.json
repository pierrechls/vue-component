{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "{{ gitRepoUrl }}"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue-component"
  ],
  "author": {
    "name": "{{ author }}",
    "email": "{{ authorEmail }}",
    "url": "{{ authorUrl }}"
  },
  "license": "MIT",
  "bugs": {
    "url": "{{ gitRepoUrl }}/issues"
  },
  "homepage": "{{ gitRepoUrl }}#readme",
